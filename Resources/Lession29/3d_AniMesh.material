// A "Material" file can contain one or more materials
material orcMaterial
{
	technique normal
	{

		pass 0
		{
			shader
			{
				vertexShader = Shaders3D/Lession29.vert
				fragmentShader = Shaders3D/Lession29.frag
				// sampler:
				//   responsible for setting the texture and its parameters
				//   the Id of the sampler is the uniform name
				sampler u_sampler0
				{
					path = monguger.tga
				}
			}
		}
	}
}